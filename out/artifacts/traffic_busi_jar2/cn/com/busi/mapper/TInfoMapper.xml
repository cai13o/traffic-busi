    <insert id="inserTInfo" parameterType="cn.com.busi.entity.TInfo">
        INSERT INTO t_info
         ( id,jylsh,jyjgbh,jyjgmc,jcxdh,wtr,dlyszh,jylb,ywlx,jcxb,jcrq,jcjldbh,cphm,hpzl,gcpzh,gclx,clccrq,wjdly,ycy,wqczy,zcdjrq,vin,clxh,fdjh,csys,xszlc,qdxs,zxzxjxs,gczs,zdjygw,dgjygw,dlxjygw,t_detail)
         VALUES (
          #{id},#{jylsh},#{jyjgbh},#{jyjgmc},#{jcxdh},#{wtr},#{dlyszh},#{jylb},#{ywlx},#{jcxb},#{jcrq},#{jcjldbh},#{cphm},#{hpzl},#{gcpzh},#{gclx},#{clccrq},#{wjdly},#{ycy},#{wqczy},#{zcdjrq},#{vin},#{clxh},#{fdjh},#{csys},#{xszlc},#{qdxs},#{zxzxjxs},#{gczs},#{zdjygw},#{dgjygw},#{dlxjygw},#{tDetail}
