        <id column="id" property="id"/>
        <result column="jylsh" property="jylsh"/>
        <result column="jyjgbh" property="jyjgbh"/>
        <result column="jyjgmc" property="jyjgmc"/>
        <result column="jcxdh" property="jcxdh"/>
        <result column="wtr" property="wtr"/>
        <result column="dlyszh" property="dlyszh"/>
        <result column="jylb" property="jylb"/>
        <result column="ywlx" property="ywlx"/>
        <result column="jcxb" property="jcxb"/>
        <result column="jcrq" property="jcrq"/>
        <result column="jcjldbh" property="jcjldbh"/>
        <result column="cphm" property="cphm"/>
        <result column="hpzl" property="hpzl"/>
        <result column="gcpzh" property="gcpzh"/>
        <result column="gclx" property="gclx"/>
        <result column="clccrq" property="clccrq"/>
        <result column="wjdly" property="wjdly"/>
        <result column="ycy" property="ycy"/>
        <result column="wqczy" property="wqczy"/>
