<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.com.busi.mapper.TInstInfoDao">

    <resultMap id="BaseResultMap" type="cn.com.busi.entity.TInstInfo">
        <!--@Table t_inst_info-->
        <result property="id" column="id" jdbcType="VARCHAR"/>
        <result property="name" column="name" jdbcType="VARCHAR"/>
        <result property="owner" column="owner" jdbcType="VARCHAR"/>
        <result property="phone" column="phone" jdbcType="VARCHAR"/>
        <result property="contact" column="contact" jdbcType="VARCHAR"/>
        <result property="area" column="area" jdbcType="VARCHAR"/>
        <result property="address" column="address" jdbcType="VARCHAR"/>
        <result property="certificate" column="certificate" jdbcType="VARCHAR"/>
        <result property="startdate" column="startDate" jdbcType="VARCHAR"/>
        <result property="enddate" column="endDate" jdbcType="VARCHAR"/>
        <result property="opflag" column="opFlag" jdbcType="VARCHAR"/>
        <result property="opdate" column="opDate" jdbcType="VARCHAR"/>
    </resultMap>

    <!--查询单个-->
    <select id="queryById" resultMap="BaseResultMap">
        select
          id, name, owner, phone, contact, area, address, certificate, startDate, endDate, opFlag, opDate
        from t_inst_info
        where id = #{id}
    </select>

    <!--查询指定行数据-->
    <select id="queryAllByLimit" resultMap="BaseResultMap">
        select
          id, name, owner, phone, contact, area, address, certificate, startDate, endDate, opFlag, opDate
        from t_inst_info
        limit #{offset}, #{limit}
    </select>

    <!--通过实体作为筛选条件查询-->
    <select id="queryAll" resultMap="BaseResultMap">
        select
          id, name, owner, phone, contact, area, address, certificate, startDate, endDate, opFlag, opDate
        from t_inst_info
        <where>
            <if test="id != null and id != ''">
                and id = #{id}
            </if>
            <if test="name != null and name != ''">
                and name like concat(concat('%',#{name}),'%')
            </if>
            <if test="owner != null and owner != ''">
                and owner like concat(concat('%',#{owner}),'%')
            </if>
            <if test="phone != null and phone != ''">
                and phone = #{phone}
            </if>
            <if test="contact != null and contact != ''">
                and contact = #{contact}
            </if>
            <if test="area != null and area != ''">
                and area like concat(concat('%',#{area}),'%')
            </if>
            <if test="address != null and address != ''">
                and address = #{address}
            </if>
            <if test="certificate != null and certificate != ''">
                and certificate = #{certificate}
            </if>
            <if test="startdate != null and startdate != ''">
                and startDate = #{startdate}
            </if>
            <if test="enddate != null and enddate != ''">
                and endDate = #{enddate}
            </if>
            <if test="opflag != null and opflag != ''">
                and opFlag = #{opflag}
            </if>
            <if test="opdate != null and opdate != ''">
                and opDate = #{opdate}
            </if>
        </where>
    </select>

    <!--新增所有列-->
    <insert id="insert" keyProperty="id" useGeneratedKeys="false">
        insert into t_inst_info(id,name, owner, phone, contact, area, address, certificate, startDate, endDate, opFlag, opDate)
        values (#{id},#{name}, #{owner}, #{phone}, #{contact}, #{area}, #{address}, #{certificate}, #{startdate}, #{enddate}, #{opflag}, #{opdate})
    </insert>

    <!--通过主键修改数据-->
    <update id="update">
        update t_inst_info
        <set>
            <if test="name != null and name != ''">
                name = #{name},
            </if>
            <if test="owner != null and owner != ''">
                owner = #{owner},
            </if>
            <if test="phone != null and phone != ''">
                phone = #{phone},
            </if>
            <if test="contact != null and contact != ''">
                contact = #{contact},
            </if>
            <if test="area != null and area != ''">
                area = #{area},
            </if>
            <if test="address != null and address != ''">
                address = #{address},
            </if>
            <if test="certificate != null and certificate != ''">
                certificate = #{certificate},
            </if>
            <if test="startdate != null and startdate != ''">
                startDate = #{startdate},
            </if>
            <if test="enddate != null and enddate != ''">
                endDate = #{enddate},
            </if>
            <if test="opflag != null and opflag != ''">
                opFlag = #{opflag},
            </if>
            <if test="opdate != null and opdate != ''">
                opDate = #{opdate},
            </if>
        </set>
        where id = #{id}
    </update>

    <!--通过主键删除-->
    <delete id="deleteById">
        delete from t_inst_info where id = #{id}
    </delete>

    <select id="count" resultMap="BaseResultMap">
        select
          count(id) name
        from t_inst_info
    </select>

</mapper>